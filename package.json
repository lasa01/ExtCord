    "@types/chai": "^4.2.0",
    "@types/fs-extra": "^5.1.0",
    "@types/hjson": "^2.4.1",
    "@types/mocha": "^5.2.7",
    "@types/node": "^8.10.49",
    "@types/string-format": "^2.0.0",
    "@types/yargs": "^12.0.12",
    "chai": "^4.2.0",
    "mocha": "^6.2.0",
    "ts-node": "^8.3.0",
    "tslint": "^5.18.0",
