{
  "name": "extcord",
  "version": "0.0.1",
  "description": "Discord bot",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/extcord.js",
    "prepare": "npm run build",
    "build": "tsc -b src_modules",
    "build:bot": "tsc -b src",
    "typedoc": "cd src && typedoc",
    "test": "mocha",
    "lint": "tslint -p .",
    "clear:languages": "rimraf ./languages",
    "clear:privileges": "rimraf ./privileges",
    "clear": "npm run clear:languages && npm run clear:privileges"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lasa01/ExtCord.git"
  },
  "author": "lasa01",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/lasa01/ExtCord/issues"
  },
  "homepage": "https://github.com/lasa01/ExtCord#readme",
  "engines": {
    "node": ">=16.0.0"
  },
  "bin": {
    "extcord": "./bin/extcord"
  },
  "files": [
    "dist",
    "modules",
    "bin"
  ],
  "mocha": {
    "extension": [
      "ts"
    ],
    "spec": "tests/**/*.spec.ts",
    "require": "ts-node/register"
  },
  "dependencies": {
    "@discordjs/builders": "^0.6.0",
    "@discordjs/opus": "^0.3.3",
    "@discordjs/rest": "^0.1.0-canary.0",
    "@discordjs/voice": "^0.6.0",
    "bufferutil": "^4.0.3",
    "discord-api-types": "^0.22.0",
    "discord.js": "^13.1.0",
    "erlpack": "^0.1.3",
    "fs-extra": "^7.0.1",
    "hjson": "^3.2.1",
    "mysql": "^2.18.1",
    "node-fetch": "^2.6.1",
    "reflect-metadata": "^0.1.13",
    "sodium": "^3.0.2",
    "sqlite3": "^5.0.2",
    "string-format": "^2.0.0",
    "typeorm": "^0.2.29",
    "utf-8-validate": "^5.0.5",
    "winston": "^3.3.3",
    "yargs": "^17.1.1",
    "zlib-sync": "^0.1.7"
  },
  "devDependencies": {
    "@types/chai": "^4.2.14",
    "@types/fs-extra": "^5.1.0",
    "@types/hjson": "^2.4.2",
    "@types/mocha": "^5.2.7",
    "@types/node": "^16.7.10",
    "@types/node-fetch": "^2.5.8",
    "@types/string-format": "^2.0.0",
    "@types/ws": "^7.2.9",
    "@types/yargs": "^17.0.2",
    "chai": "^4.2.0",
    "ffmpeg-static": "^4.2.7",
    "mocha": "^6.2.3",
    "rimraf": "^3.0.2",
    "ts-node": "^8.10.2",
    "tslint": "^5.20.1",
    "typedoc": "^0.21.9",
    "typescript": "^4.4.2",
    "ytdl-core": "^4.9.1",
    "ytpl": "^2.2.3",
    "ytsr": "^3.5.3"
  }
}
